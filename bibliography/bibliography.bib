  issn    = {1461-0248},
  journal = {Ecology letters},
  keywords  = {2011,carbon cycle,data assimilation,earth system
      modelling,ecological forecasting,ecology letters,global
      climate change,inverse,modelling,palaeoclimatic
      reconstruction,sequential data assimilation,variational
      data assimilation},
  month   = mar,
  pmid    = {21366814},
  title   = {{Integrating models with data in ecology and
      palaeoecology: advances towards a model-data fusion
      approach.}},
  url   = {http://www.ncbi.nlm.nih.gov/pubmed/21366814},
