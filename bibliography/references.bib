  issn    = {1461-0248},
  journal = {Ecology letters},
  keywords  = {Age Factors,Animals,Biological,Computer
      Simulation,Ecosystem,Life Cycle Stages,Life Cycle Stages:
      physiology,Models,Population Dynamics},
  month   = jun,
  number  = {6},
  pages   = {576--87},
  pmid    = {18371091},
  title   = {{Crossing habitat boundaries: coupling dynamics of
      ecosystems through complex life cycles.}},
