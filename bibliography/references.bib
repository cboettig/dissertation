  keywords  = {Ancient,Climate,Forecasting,Geology,Greenhouse
      Effect,History,Models,Periodicity,Theoretical,Time
      Factors,critical transition,ecology,leading
      indicator,regime shifts,stochastic,tipping point,warning
      signals},
  mendeley-tags = {critical transition,ecology,leading indicator,regime
      shifts,stochastic,tipping point,warning signals},
  month   = sep,
  number  = {38},
